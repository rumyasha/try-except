class Car:
    def __init__(self, brand, model, year):
        self.brand = brand
        self.model = model
        self.year = year
get_info = Car("BMW", "X5 E70", "2013")
print(get_info.brand)
print("------" * 20) #это для красоты
print(get_info.model)
print("------" * 20) #это для красоты 
print(get_info.year)
